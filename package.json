{
    "name": "aoe2-2v1",
    "version": "0.1.0",
    "description": "An application to play 2v1 matches (or other configurations) with extra challenges and commands!",
    "author": "KSneijders & Prelusion",
    "main": "./electron/main.js",
    "build": {
        "appId": "aoe2.2v1.app",
        "directories": {
            "output": "dist",
            "app": "./"
        },
        "target": [
            "nsis"
        ]
    },
    "scripts": {
        "start": "electron ./electron/main.js",
        "build:dev": "vue-cli-service build",
        "build:dev:watch": "vue-cli-service build --watch",
        "build:prod": "vue-cli-service build --modern --production",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "dependencies": {
        "axios": "^0.21.2",
        "core-js": "^3.6.5",
        "dotenv": "^8.2.0",
        "vue": "^3.0.0",
        "vue-class-component": "^8.0.0-0",
        "vuex": "^4.0.0-0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^5.2.4",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-mocha": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^2.0.0-0",
        "chai": "^4.1.2",
        "electron": "^11.0.3",
        "electron-builder": "^22.13.1",
        "electron-packager": "^15.4.0",
        "electron-reloader": "^1.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0-0",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "typescript": "^4.3.5"
    }
}
